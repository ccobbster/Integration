<?xml version="1.0"?>
<!DOCTYPE hibernate-mapping PUBLIC
      "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
          "http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">

<hibernate-mapping package="com.versionone.git">
<class name="PersistentChange" table="PersistentChange">
    <id name="id" column="Id">
        <generator class="org.hibernate.id.TableHiLoGenerator"></generator>
    </id>
    <property name="hash" unique="true" not-null="true" column="Hash" length="32" />
</class>
</hibernate-mapping>